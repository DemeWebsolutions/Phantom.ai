{
  "name": "demewebsolutions/phantom-ai",
  "description": "Phantom AI tooling for WordPress plugin compliance and automated remediation with tiered workflow automation",
  "type": "project",
  "keywords": ["wordpress", "ai", "workflow", "automation", "copilot", "compliance"],
  "require": {
    "php": ">=7.4",
    "nikic/php-parser": "^4.15"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.7",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
    "phpstan/phpstan": "^1.12"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "autoload": {
    "psr-4": {
      "PhantomAI\\": "phantom-ai/"
    }
  },
  "bin": [
    "phantom-ai/phantom-cli"
  ],
  "scripts": {
    "phantom": "phantom-ai/phantom-cli"
  }
}
